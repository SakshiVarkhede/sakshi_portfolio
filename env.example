# SendGrid API Configuration
# Copy this file to .env.local and fill in your actual values

# Your SendGrid API Key (get it from https://app.sendgrid.com/settings/api_keys)
SENDGRID_API_KEY=your-sendgrid-api-key-here

# Email address where you want to receive contact form messages
SENDGRID_EMAIL_TO=sakshi@example.com

# Email address to send from (can be same as SENDGRID_EMAIL_TO, or use a verified sender)
# This must be a verified sender in SendGrid
SENDGRID_EMAIL_FROM=sakshi@example.com

